<build>
    <plugins>
        <plugin>
            <groupId>org.apache.axis2</groupId>
            <artifactId>axis2-aar-maven-plugin</artifactId>
            <version>1.4</version>
            <extensions>true</extensions>
            <configuration>
                <servicesXmlFile>${basedir}/resources/META-INF/services.xml</servicesXmlFile>
                <wsdlFile>${basedir}/resources/META-INF/EPSManager.wsdl</wsdlFile>
				<fileSets> 
                    <fileSet>
                        <directory>${basedir}/data</directory>
                        <outputDirectory>data</outputDirectory>
                        <includes>
                            <include>*.xml</include>
                        </includes>
                    </fileSet>
                </fileSets>
